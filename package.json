{
  "name": "mui-studio",
  "version": "0.0.1",
  "description": "Build MUI apps quickly",
  "repository": "https://github.com/mui-org/mui-studio.git",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "docs"
    ]
  },
  "scripts": {
    "prettier": "prettier --check .",
    "start": "cross-env FORCE_COLOR=1 lerna run --stream --scope @mui/studio start",
    "dev": "cross-env FORCE_COLOR=1 lerna run --stream --scope @mui/studio dev",
    "build": "lerna run --stream build"
  },
  "devDependencies": {
    "@material-ui/monorepo": "https://github.com/mui-org/material-ui.git#master",
    "@swc/core": "^1.2.108",
    "@swc/helpers": "^0.2.14",
    "@types/node": "^16.11.7",
    "cross-env": "^7.0.3",
    "lerna": "^4.0.0",
    "prettier": "^2.4.1",
    "regenerator-runtime": "^0.13.9",
    "typescript": "^4.4.4"
  }
}
